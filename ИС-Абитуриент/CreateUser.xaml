<Window x:Class="ИС_Абитуриент.CreateUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ИС_Абитуриент"
        mc:Ignorable="d"
        Title="CreateUser" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="178,174,0,0" TextWrapping="Wrap" Text="Login" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="325,174,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Создать" HorizontalAlignment="Left" Margin="392,237,0,0" VerticalAlignment="Top" Click="button_Click"/>
        <CheckBox x:Name="checkBox" Content="Admin" HorizontalAlignment="Left" Margin="508,177,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="checkBox1" Content="Employee" HorizontalAlignment="Left" Margin="609,177,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
