<Window x:Class="ИС_Абитуриент.ExamResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ИС_Абитуриент"
        mc:Ignorable="d"
        Title="ExamResult" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,161,0,0" IsReadOnly="True" CanUserAddRows="False" CanUserDeleteRows="False"/>
        <Button x:Name="button" Content="Сохранить отчет" HorizontalAlignment="Left" Margin="26,55,0,0" VerticalAlignment="Top" Click="button_Click"/>

    </Grid>
</Window>
